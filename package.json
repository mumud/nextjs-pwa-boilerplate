{
  "name": "nextjs-pwa-boilerplate",
  "version": "0.1.0",
  "description": "Next JS PWA Boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/mumud/nextjs-pwa-boilerplate.git"
  },
  "author": "Mumud Halimudin",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "keywords": [
    "nextjs",
    "pwa",
    "tailwindcss",
    "eslintrc",
    "prettierrc"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
    "next": "10.2.3",
    "next-pwa": "^5.2.21",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "husky": "^6.0.0",
    "i": "^0.3.6",
    "lint-staged": "^11.0.0",
    "npm": "^7.15.0",
    "postcss": "^8.2.15",
    "prettier": "^2.3.0",
    "stylelint-config-recommended": "^5.0.0",
    "tailwindcss": "^2.1.2"
  }
}
